<application>
  <component name="SqlEditorOptions">
    <option name="concatenateStringsOnEnter" value="true" />
    <option name="qualificationType" value="On collisions" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern pattern="\bfixme\b.*" />
    <pattern icon="important" useCustomColors="true" pattern="\bmodify\b.*">
      <option name="FOREGROUND" value="c05600" />
      <option name="FONT_TYPE" value="2" />
      <option name="ERROR_STRIPE_COLOR" value="ab9300" />
    </pattern>
    <pattern useCustomColors="true" pattern="\bimplement\b.*">
      <option name="FOREGROUND" value="1195c0" />
      <option name="FONT_TYPE" value="2" />
      <option name="ERROR_STRIPE_COLOR" value="ab9319" />
    </pattern>
    <pattern useCustomColors="true" pattern="\bspec\b.*">
      <option name="FOREGROUND" value="8ec084" />
      <option name="FONT_TYPE" value="2" />
      <option name="ERROR_STRIPE_COLOR" value="ab9319" />
    </pattern>
  </component>
</application>