<application>
  <component name="SqlEditorOptions">
    <option name="concatenateStringsOnEnter" value="true" />
    <option name="qualificationType" value="On collisions" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern pattern="\bfixme\b.*" />
    <pattern icon="question" useCustomColors="true" pattern="\bnote\b.*">
      <option name="FOREGROUND" value="156501" />
      <option name="FONT_TYPE" value="2" />
      <option name="ERROR_STRIPE_COLOR" value="ab7817" />
    </pattern>
    <pattern icon="important" useCustomColors="true" pattern="\bmodification\b.*">
      <option name="FOREGROUND" value="c05600" />
      <option name="FONT_TYPE" value="2" />
      <option name="ERROR_STRIPE_COLOR" value="ab9300" />
    </pattern>
  </component>
</application>